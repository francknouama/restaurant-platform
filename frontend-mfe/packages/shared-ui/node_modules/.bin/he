#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/franck/reactive-crafters-workspace/restaurant-platform/frontend-mfe/packages/shared-ui/node_modules/he/bin/node_modules:/Users/franck/reactive-crafters-workspace/restaurant-platform/frontend-mfe/packages/shared-ui/node_modules/he/node_modules:/Users/franck/reactive-crafters-workspace/restaurant-platform/frontend-mfe/packages/shared-ui/node_modules:/Users/franck/reactive-crafters-workspace/restaurant-platform/frontend-mfe/packages/node_modules:/Users/franck/reactive-crafters-workspace/restaurant-platform/frontend-mfe/node_modules:/Users/franck/reactive-crafters-workspace/restaurant-platform/node_modules:/Users/franck/reactive-crafters-workspace/node_modules:/Users/franck/node_modules:/Users/node_modules:/node_modules:/Users/franck/reactive-crafters-workspace/restaurant-platform/frontend-mfe/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/franck/reactive-crafters-workspace/restaurant-platform/frontend-mfe/packages/shared-ui/node_modules/he/bin/node_modules:/Users/franck/reactive-crafters-workspace/restaurant-platform/frontend-mfe/packages/shared-ui/node_modules/he/node_modules:/Users/franck/reactive-crafters-workspace/restaurant-platform/frontend-mfe/packages/shared-ui/node_modules:/Users/franck/reactive-crafters-workspace/restaurant-platform/frontend-mfe/packages/node_modules:/Users/franck/reactive-crafters-workspace/restaurant-platform/frontend-mfe/node_modules:/Users/franck/reactive-crafters-workspace/restaurant-platform/node_modules:/Users/franck/reactive-crafters-workspace/node_modules:/Users/franck/node_modules:/Users/node_modules:/node_modules:/Users/franck/reactive-crafters-workspace/restaurant-platform/frontend-mfe/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../he/bin/he" "$@"
else
  exec node  "$basedir/../he/bin/he" "$@"
fi
